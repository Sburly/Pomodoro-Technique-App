<% layout('layout') %>
<div class="row err">
    <div class="col-4 offset-4">
        <div class="alert alert-danger" role="alert">
            <h4 class="alert-heading"><%= err.message %></h4>
            <% if(process.env.NODE_ENV !== "production") { %> 
                <p><%= err.stack %></p>
            <% } %>
        </div>
    </div>
</div>